{
  "dry_run": true,
  "dry_run_wallet": 100,
  "max_open_trades": 1,
  "stake_currency": "USDT",
  "stake_amount": 10,
  "timeframe": "15m",
  "exchange": {
    "name": "kraken",
    "ccxt_config": {
      "enableRateLimit": true
    },
    "ccxt_async_config": {
      "enableRateLimit": true
    },
    "pair_whitelist": [
      "BTC/USDT",
      "ETH/USDT"
    ],
    "pair_blacklist": []
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  "entry_pricing": {
    "price_side": "ask",
    "use_order_book": false,
    "fallback": "last"
  },
  "exit_pricing": {
    "price_side": "bid",
    "use_order_book": false,
    "fallback": "last"
  },
  "stoploss": -0.02,
  "minimal_roi": {
    "0": 0.01
  },
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "verbosity": "error",
    "enable_openapi": false,
    "username": "admin",
    "password": "changeme",
    "jwt_secret_key": "CHANGE_THIS_TO_A_RANDOM_LONG_STRING",
    "CORS_origins": [
      "http://localhost:8080",
      "http://localhost:3000"
    ]
  },
  "bot_name": "local-basic",
  "runmode": "live",
  "force_entry_enable": true
}
